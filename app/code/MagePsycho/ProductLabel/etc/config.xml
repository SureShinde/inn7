<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <magepsycho_productlabel>
            <general>
                <enabled>1</enabled>
                <debug>0</debug>
            </general>
            <discount>
                <format><![CDATA[{regular_price} - {discount_percent}% = {final_price}]]></format>
            </discount>
        </magepsycho_productlabel>
    </default>
</config>
